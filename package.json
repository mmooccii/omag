{
  "name": "omag_template",
  "version": "1.0.0",
  "description": "Working templates",
  "main": "index.js",
  "scripts": {
    "setup": "npm ci",
    "start": "cross-env NODE_ENV=development webpack serve --mode development --hot --liveReload",
    "build": "cross-env NODE_ENV=development webpack --mode development -w",
    "prod": "cross-env NODE_ENV=production webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmooccii/omag.git"
  },
  "keywords": ["omg", "2021", "autumn"],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mmooccii/omag/issues"
  },
  "homepage": "https://github.com/mmooccii/omag#readme",
  "browserslist": ["last 5 versions", "not ie 6-10"],
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "parser": "json"
        }
      },
      {
        "files": "*.html",
        "options": {
          "parser": "html"
        }
      },
      {
        "files": "*.css",
        "options": {
          "parser": "css"
        }
      },
      {
        "files": "*.scss",
        "options": {
          "parser": "scss"
        }
      }
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@mmooccii/lazyload-loader": "^1.1.3",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-mqpacker": "^7.0.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "mini-css-extract-plugin": "^1.6.2",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.1.1",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "styles-loader": "^3.0.0",
    "svg-inline-loader": "^0.8.2",
    "svg-url-loader": "^7.1.1",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-remove-empty-scripts": "^0.7.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "core-js": "^3.15.2",
    "detect-browser": "^5.2.0",
    "emitter-js": "^2.0.2",
    "gsap": "^3.7.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.7",
    "swiper": "^6.7.5",
    "vanilla-lazyload": "^17.4.0"
  }
}
